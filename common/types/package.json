{
  "name": "@freedman-trucking/types",
  "version": "1.0.0",
  "description": "Shared entities between webapp and backend function",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.mjs.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "dev": "tsc -w",
    "pack": "npm run build && npm pack",
    "watch": "tsc -w"
  },
  "dependencies": {
    "arktype": "^2.1.10"
  }
}
